class Solution:
    def isPalindrome(self, x: int) -> bool:
        l = 0
        r = len(str(x)) - 1
        # x = str(x)
        # print(l,r,x)
        while l<=r:
            if str(x)[l]!=str(x)[r]:
                return False
            else :
                l=l+1
                r=r-1
        return True
