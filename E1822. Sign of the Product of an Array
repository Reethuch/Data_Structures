class Solution:
    def signFunc(self, a: int) -> int:
        if a > 0:
            return 1
        if a < 0:
            return -1
        if a == 0:
            return 0
        
    def arraySign(self, nums: List[int]) -> int:
        a = 1
        for i in nums:
            a *= i
        return Solution.signFunc(self, a)
        

       
