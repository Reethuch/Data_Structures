class Solution:
    def triangularSum(self, nums: List[int]) -> int:
        loop = 0
        k = len(nums)
        while k!=1:
            ll = [0]*(k-1)
            for i in range(k-1):
                ll[i] = nums[i] + nums[i+1]
                if ll[i]>=10:
                    ll[i]-=10
            nums[:] = ll[:]
            k-=1
        return nums[0]
        
